{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Micorsoft": "Warning",
        "Micorsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      },

      {
        "Name": "Seq",
        "Args":  { "serverUrl":  "http://localhost:5341"}
      }
    ]
  },
  "AllowedHosts": "*"
}
